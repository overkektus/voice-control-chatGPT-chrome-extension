{
  "name": "Voice Control for chatGPT",
  "version": "1.0.0",
  "description": "This new browser extension changes how you talk to ChatGPT. You can now have voice conversations with it easily. The extension makes it simple and smooth to chat with ChatGPT using your voice. It's a helpful tool to make your experience with advanced AI better. Try it now to upgrade your AI interactions!",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonghakseo/chrome-extension-boilerplate-react-vite.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build --watch",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.2",
    "@types/react-speech-recognition": "^3.9.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-speech-recognition": "^3.10.0",
    "regenerator-runtime": "^0.13.11"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.224",
    "@types/jest": "29.0.3",
    "@types/node": "20.5.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.2.7",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "2.2.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "8.49.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "fs-extra": "11.1.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "rollup": "2.79.1",
    "sass": "1.55.0",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.4",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "ws": "8.13.0"
  }
}
